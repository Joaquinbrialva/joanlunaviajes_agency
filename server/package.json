{
	"name": "joanluna-agency",
	"version": "1.0.0",
	"description": "Backend API para JoanlunaViajes - gesti√≥n de agencia de viajes",
	"license": "MIT",
	"author": "Joaquin Alvarez",
	"type": "commonjs",
	"main": "src/index.js",
	"scripts": {
		"dev": "nodemon src/index.js",
		"test": "jest",
		"migrations:generate": "sequelize-cli migration:generate --name",
		"migrations:run": "sequelize-cli db:migrate",
		"migrations:revert": "sequelize-cli db:migrate:undo",
		"migrations:delete": "sequelize-cli db:migrate:undo:all",
		"migrations:revert:name": "sequelize-cli db:migrate:undo --name",
		"seed:generate": "sequelize-cli seed:generate --name",
		"seed:run": "sequelize-cli db:seed:all",
		"seed:run:name": "sequelize-cli db:seed --seed",
		"seed:revert": "sequelize-cli db:seed:undo",
		"seed:revert:name": "sequelize-cli db:seed:undo --name"
	},
	"devDependencies": {
		"jest": "^30.2.0",
		"morgan": "^1.10.1",
		"nodemon": "^3.1.10",
		"sequelize-cli": "^6.6.3"
	},
	"dependencies": {
		"@hapi/boom": "^10.0.1",
		"bcrypt": "^6.0.0",
		"cors": "^2.8.5",
		"dotenv": "^17.2.1",
		"express": "^5.1.0",
		"joi": "^18.0.1",
		"js-yaml": "^4.1.0",
		"jsonwebtoken": "^9.0.2",
		"multer": "^2.0.2",
		"passport": "^0.7.0",
		"passport-jwt": "^4.0.1",
		"pg": "^8.16.3",
		"sequelize": "^6.37.7",
